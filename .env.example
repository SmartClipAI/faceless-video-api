# OpenAI Configuration (Default)
OPENAI_BASE_URL="your-openai-base-url"
OPENAI_API_KEY="your-openai-api-key"

# Azure OpenAI Configuration (Optional)
# Uncomment and configure these if using Azure OpenAI
# AZURE_OPENAI_ENDPOINT="https://your-azure-openai-endpoint/"
# AZURE_OPENAI_API_KEY="your-azure-openai-key"

# AI Service Keys (Choose one)
# Option 1: FAL (Default)
FAL_KEY="your-fal-key"
# Option 2: Replicate (Alternative)
# REPLICATE_API_TOKEN="your-replicate-token"

# Database Configuration
# Format: postgresql://username:password@host:port/database_name
DATABASE_URL="postgresql://user:password@localhost:5432/faceless_api_dev"

# Authentication
# Generate a secure random string for SECRET_KEY
SECRET_KEY="your-secret-key"

# Admin User Configuration
ADMIN_USERNAME="admin"
ADMIN_EMAIL="admin@example.com"
ADMIN_PASSWORD="your-secure-password"

# Storage Configuration (Cloudflare R2)
R2_BUCKET_NAME="your-bucket-name"
R2_ENDPOINT="https://your-r2-endpoint.r2.cloudflarestorage.com"
R2_PUBLIC_ENDPOINT="https://your-public-endpoint.r2.dev"
R2_ACCESS_KEY_ID="your-r2-access-key-id"
R2_SECRET_ACCESS_KEY="your-r2-secret-access-key" 